{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "anaseqal/nova-import": "^0.0.3",
        "artesaos/seotools": "^0.20.2",
        "awssat/laravel-visits": "^2.2",
        "ctessier/nova-advanced-image-field": "^1.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8.1",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/agent": "^2.6",
        "jfeid/nova-google-maps": "^1.1",
        "kirschbaum-development/nova-mail": "^0.1.0",
        "laravel/framework": "^7.0",
        "laravel/nova": "*",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "lexxyungcarter/chatmessenger": "^1.3",
        "metrixinfo/nova-iframe": "^1.0",
        "overtrue/laravel-follow": "^2.1",
        "pktharindu/nova-permissions": "^2.1",
        "pusher/pusher-php-server": "~4.0",
        "wasandev/input-thai-address": "*",
        "wasandev/user-card": "*"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/InputThaiAddress"
        },
        {
            "type": "path",
            "url": "./nova-components/UserCard"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish"
        ]
    }
}
